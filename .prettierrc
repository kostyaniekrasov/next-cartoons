{
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "printWidth": 80,
  "tabWidth": 2,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "prettier-plugin-organize-imports"
  ],
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "printWidth": 80
      }
    },
    {
      "files": "*.scss",
      "options": {
        "printWidth": 80
      }
    }
  ]
}
